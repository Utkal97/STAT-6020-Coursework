library(fortunes)   # Load fortunes library
help(package="fortunes")  #get help description for fortunes package
ls("package:fortunes") #list contents of fortunes package
help(package="fortunes")  #get help description for fortunes package
fortune("Ripley") #show some text containing Ripley
fortune(17) #not implemented yet



library(cowsay)   # Load fortunes library
help(package="cowsay")  #get help description for cowsay package
ls("package:cowsay") #list contents of fortunes package
animals #show animals vector
names(animals) #shows names in animals vector
say() #say default message
say(by="cow") #say message by cow
say("GO TIGERS!", by="chicken") #say custom message by chicken
say(paste(fortune(), collapse="\n"), by="monkey") #say random text by monkey
say(paste(fortune(), collapse="\n"), by="monkey") #say another random text by monkey
data() #display datasets
?mtcars #show description of mtcars dataset in help
head(mtcars, 8) #show first 8 rows of mtcars
dplyr::glimpse(mtcars) #show a gist of mtcars
nrow(mtcars) #number of rows in mtcars
?tidyr::billboard #show description of billboard dataset in tidyr package
dplyr::glimpse(tidyr::billboard) #show gist of billboard dataset in tidyr package
